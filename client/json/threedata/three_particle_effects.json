            {"process":"valueToQuat",   "source":"age"        , "target":"orientation"  },
            {"process":"attrib4D",      "source":"orientation", "target":"orientation"  },
            {"process":"attrib3D",      "source":"position"   , "target":"offset"       },
            {"process":"curve1DtoValue","source":"sizeCurve"  , "target":"size"         },
            {"process":"attrib2D",      "source":"tiles"      , "target":"tileindex"    },
