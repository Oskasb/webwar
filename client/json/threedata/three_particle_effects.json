[
  {
    "PARTICLE_EFFECTS": {
      "THREE":[
        {
          "id":"test_effect",
          "renderer_id":"opaque_atlas_renderer",
          "particle_key":"THREE",
          "particle_id":"default",
          "count":3,
          "init_params":[
            {"param":"age",         "value":  {"min":0, "max":0}},
            {"param":"lifeTime",    "value":  {"min":1, "max":4}},
            {"param":"size",        "value":  {"min":0, "max":0.5}},
            {"param":"tiles",       "vec2":   {"x":1,   "y":5}},
            {"param":"position",    "vec3":   {"x":0,   "y":1,   "z":0,          "spread":{"min":-0.5, "max":0.5}}},
            {"param":"offset",      "vec3":   {"x":0,   "y":0,   "z":0,          "spread":{"min":-0.2, "max":0.2}}},
            {"param":"orientation", "quat":   {"x":0,   "y":1,   "z":0,  "w":1,  "spread":{"min":-0.5, "max":0.5}}},
            {"param":"velocity",    "vec3":   {"x":0,   "y":0,   "z":0,          "spread":{"min":-0.2, "max":0.2}}},
            {"param":"sizeCurve",   "curve1D":["quickInOut"], "amplitude":0.41,       "spread":{"min":-0.2, "max":0.2}},
            {"param":"colorCurve",  "curve3D":["zeroToOne", "attackIn",  "quickInOut"], "amplitudes":[0.8, 0.7, 0.5], "spread":{"min":-0.2, "max":0.2}},
            {"param":"customColor", "vec3":   {"x":0.5, "y":0.5, "z":0.5,         "spread":{"min":-0.5, "max":0.5}}}

